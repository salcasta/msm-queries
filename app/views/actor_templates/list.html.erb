<h1>List of Actorss</h1>

<table border=1>

  <% Actor.all.each do |a_actor| %>
    <tr>
      <td><%= a_actor.id %></td>
      <td>
        <img src="<%= a_actor.image %>" alt="headshot" width="100">
      </td>
      <td><%= a_actor.name %></td>
      <td><%= a_actor.dob %></td>
      <td>
        <a href="/actors/<%= a_actor.id %>">Show Details</a>
      </td>
    </tr>
  <% end %>

</table>
